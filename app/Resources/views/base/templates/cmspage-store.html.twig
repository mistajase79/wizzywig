{% extends '@theme/base.html.twig' %}
{% block title %}{{page.title}}{% endblock %}
{% block metatitle %}{{page.metatitle}}{% endblock %}
{% block metadescription %}{{page.metadescription}}{% endblock %}
{% block breadcrumbs %}{{ breadcrumbs(page, domCheckIgnore(app.request.pathinfo) ) }}{% endblock %}

{% block body %}
<!-- START templates\cmspage-store.html.twig -->
<aside class="sidebar col-xs-12 col-sm-3 col-md-4">
	{% block store_categories %}
       {{ render(controller('PrototypeCatalogBundle:Default:fetchCategoryMenu')) }}
    {% endblock %}
</aside>

<section class="main col-xs-12 col-sm-9 col-md-8" >
    <h2>Store Template</h2>
	{% block store_content %}
        {{page.content|raw}}
    {% endblock %}
</section>
<!-- END templates\cmspage-store.html.twig -->
{% endblock %}

{% block stylesheets %}{% endblock %}
{% block javascripts %}{% endblock %}
